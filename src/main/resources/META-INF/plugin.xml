<idea-plugin>
    <id>rip.deadcode.intellij-jp2en-dict-plugin</id>
    <name>Japanese2English Dictionary Plugin</name>
    <vendor email="" url=""></vendor>

    <description><![CDATA[
    Translates Japanese to English
    ]]></description>

    <change-notes><![CDATA[
    ]]></change-notes>

    <depends>com.intellij.modules.platform</depends>

    <actions>
        <action id="Jp2En.CompletionRequest" class="rip.deadcode.intellij.jp2en.Jp2EnAction" text="Jp2En" description="Translates Japanese word">
