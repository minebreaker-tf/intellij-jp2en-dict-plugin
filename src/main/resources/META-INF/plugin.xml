<idea-plugin>
    <id>rip.deadcode.intellij-jp2en-dict-plugin</id>
    <name>Japanese2English Dictionary</name>
    <version>0.1.4</version>
    <vendor email="" url="">minebreaker</vendor>

    <description><![CDATA[
    Translates Japanese to English.
    Requires 'Dictionary Core' plugin.
    ]]></description>

    <change-notes><![CDATA[
    No change notes. Still in the alpha status.
    ]]></change-notes>

    <depends>com.intellij.modules.platform</depends>
    <depends>rip.deadcode.intellij-dictionary-plugin</depends>

    <extensions defaultExtensionNs="rip.deadcode.intellij-dictionary-plugin">
        <dictionary implementation="rip.deadcode.intellij.jp2en.Jp2EnDictionary"/>
    </extensions>

</idea-plugin>
